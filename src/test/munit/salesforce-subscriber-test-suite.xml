<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="salesforce-subscriber-test-suite.xml" />
	<munit:test name="salesforce-subscriber-test-suite-anypointmq-salesforce-subscriberTest" doc:id="360b9f48-4275-437d-be0a-b10d28f48561" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when SFDC platform event" doc:id="c5eb8cf0-1985-4f32-bace-c7b550715fb3" processor="salesforce:publish-platform-event-message">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="Publish platform event message: Lead" attributeName="doc:name" />
					<munit-tools:with-attribute whereValue="9e65ae60-d2b8-4a32-a34c-9e5e9865a774" attributeName="doc:id" />
					<munit-tools:with-attribute whereValue="OT_Lead_Consent__e" attributeName="platformEventName" />
				</munit-tools:with-attributes>
			</munit-tools:mock-when>
			<munit-tools:mock-when doc:name="Mock when ACK" doc:id="d1f3b430-afdf-4522-aabe-4911a01d4c38" processor="anypoint-mq:ack">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="Ack" attributeName="doc:name" />
					<munit-tools:with-attribute whereValue="eca56be2-ca70-4b7a-8a9b-f792e6d43fb3" attributeName="doc:id" />
					<munit-tools:with-attribute whereValue="Anypoint_MQ_Config-Salesforce-Clinical" attributeName="config-ref" />
				</munit-tools:with-attributes>
			</munit-tools:mock-when>
			<munit-tools:mock-when doc:name="Mock when linkidentities by email" doc:id="4f43142d-178d-4942-8c40-88235dcd778d" processor="flow-ref">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="862f38aa-3190-47a9-b17b-567fc97e5ddb" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value="#[output application/json --- &#10;&#10;readUrl('classpath://onetrust\mock_linkidentities_by_email_payload.dwl')]" />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="4b2bb0d5-681e-4254-a9e7-1bcf2b08b407" >
				<munit:payload value="#[output application/json --- &#10;&#10;readUrl('classpath://mq\mock_consentreceipt_payload.dwl')]" />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to anypointmq-salesforce-subscriber" doc:id="09ac11b1-65dd-4494-8279-d08471d625e5" name="anypointmq-salesforce-subscriber"/>
		</munit:execution>
	</munit:test>

</mule>
